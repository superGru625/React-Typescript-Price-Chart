        "onchange": "^7.1.0",
        "patch-package": "^6.4.7",
        "react": "^18.1.0",
        "react-copy-to-clipboard": "^5.1.0",
        "react-device-detect": "^2.2.2",
        "react-dom": "^18.1.0",
        "react-redux": "^8.0.2",
        "react-router-dom": "^6.3.0",
